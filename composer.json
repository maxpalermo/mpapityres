{
    "version": "1.0.0",
    "name": "mpsoft/mpapityres",
    "description": "Modulo base per la gestione pneumatici Tyre24 via API per PrestaShop 8",
    "type": "prestashop-module",
    "license": "AFL-3.0",
    "authors": [
        {
            "name": "Massimiliano Palermo",
            "email": "maxx.palermo@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MpSoft\\MpApiTyres\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "extra": {
        "installer-name": "mpapityres",
        "module": {
            "name": "MpApiTyres",
            "version": "1.0.0",
            "author": "Massimiliano Palermo",
            "author_email": "maxx.palermo@gmail.com",
            "author_url": "https://www.prestashop.maxpalermo.it",
            "license": "AFL-3.0",
            "url": "https://www.prestashop.maxpalermo.it",
            "readme": "README.md"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
